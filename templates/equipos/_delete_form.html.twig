<form method="post" action="{{ path('equipos_delete', {'nombreEquipo': equipo.nombreEquipo}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ equipo.nombreEquipo) }}">
    
    <button class="botonpeque btn" name="eliminar">Eliminar</button>
</form>
